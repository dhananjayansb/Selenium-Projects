package Testcases;

import java.util.concurrent.TimeUnit;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

public class GitTest {
	@Test
	public  void loginTest()
	{
		//Set property for browser driver
		System.setProperty("webdriver.chrome.driver", "D:\\software\\Selenium\\chromedriver.exe");
		System.setProperty("webdriver.gecko.driver","D:\\software\\Selenium\\geckodriver.exe");
		
		//Launch browser
		//WebDriver driver = new ChromeDriver();
		WebDriver driver = new FirefoxDriver();
		driver.manage().window().maximize();
		driver.manage().deleteAllCookies();
		driver.manage().timeouts().pageLoadTimeout(40, TimeUnit.SECONDS);
		driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS);
		
		
		//Goto site
		try 
		{
		driver.get("https://www.github.com");
		Thread.sleep(5000);
		driver.findElement(By.xpath("/html/body/div[1]/header/div/div[2]/div[2]/div[2]/a")).click();	
		driver.findElement(By.id("login_field")).sendKeys("dhananjayansb");
		Thread.sleep(1000);
		driver.findElement(By.id("password")).clear();
		driver.findElement(By.id("password")).sendKeys("Forevadj@26");
		Thread.sleep(1000);
		driver.findElement(By.name("commit")).click();
		Thread.sleep(1000);
		}
		catch(Exception e) {};
		System.out.println("Successfully Task completed");
		
	}
}
